{
    "headers": [
        {
            "source": "/*",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "Cache-Control",
                    "value": "max-age=7200"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, HEAD, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type, Authorization, X-Requested-With"
                },
                {
                    "key": "Access-Control-Max-Age",
                    "value": "86400"
                }
            ]
        },
        {
            "source": "/avatar/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000"
                }
            ]
        },
        {
            "source": "/image/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000"
                }
            ]
        },
        {
            "source": "/Mobile/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000"
                }
            ]
        },
        {
            "source": "/PC/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000"
                }
            ]
        }
    ],
    "caches": [
        {
            "source": "/avatar/*",
            "cacheTtl": 8640000
        },
        {
            "source": "/image/*",
            "cacheTtl": 8640000
        },
        {
            "source": "/Mobile/*",
            "cacheTtl": 8640000
        },
        {
            "source": "/PC/*",
            "cacheTtl": 8640000
        }
    ]
}